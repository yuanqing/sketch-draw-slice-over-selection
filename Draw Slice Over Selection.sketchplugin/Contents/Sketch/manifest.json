{
  "name": "Draw Slice Over Selection",
  "description": "A Sketch plugin to draw a slice over the selection",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-draw-slice-over-selection",
  "version": "0.6.0",
  "identifier": "yuanqing.sketch-draw-slice-over-selection",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-draw-slice-over-selection/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Draw Slice Over Selection",
      "identifier": "yuanqing.sketch-draw-slice-over-selection.draw-slice-over-selection.draw-slice-over-selection",
      "script": "plugin.js",
      "handler": "draw-slice-over-selection/draw-slice-over-selection"
    },
    {
      "name": "Settings",
      "identifier": "yuanqing.sketch-draw-slice-over-selection.settings.settings",
      "script": "plugin.js",
      "handler": "settings/settings"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-draw-slice-over-selection.settings.reset-settings",
      "script": "plugin.js",
      "handler": "settings/reset-settings"
    }
  ],
  "menu": {
    "title": "Draw Slice Over Selection",
    "items": [
      "yuanqing.sketch-draw-slice-over-selection.draw-slice-over-selection.draw-slice-over-selection",
      "-",
      {
        "title": "Settings",
        "items": [
          "yuanqing.sketch-draw-slice-over-selection.settings.settings",
          "-",
          "yuanqing.sketch-draw-slice-over-selection.settings.reset-settings"
        ]
      }
    ]
  }
}
